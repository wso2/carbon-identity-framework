<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="org.wso2.carbon.identity.webhook.management">
    <test name="webhook-management-service-test">
        <classes>
            <class name="org.wso2.carbon.identity.webhook.management.service.WebhookManagementServiceImplTest"/>
        </classes>
    </test>
    <test name="webhook-management-dao-test">
        <classes>
            <class name="org.wso2.carbon.identity.webhook.management.dao.WebhookManagementDAOImplTest"/>
            <class name="org.wso2.carbon.identity.webhook.management.dao.CacheBackedWebhookManagementDAOTest"/>
            <class name="org.wso2.carbon.identity.webhook.management.dao.WebhookManagementDAOFacadeTest"/>
            <class name="org.wso2.carbon.identity.webhook.management.dao.PublisherAdapterTypeHandlerTest"/>
            <class name="org.wso2.carbon.identity.webhook.management.dao.WebhookAdapterTypeHandlerFactoryTest"/>
            <class name="org.wso2.carbon.identity.webhook.management.dao.PublisherSubscriberAdapterTypeHandlerTest"/>
        </classes>
    </test>
    <test name="webhook-management-internal-test">
        <classes>
            <class name="org.wso2.carbon.identity.webhook.management.internal.WebhookManagementComponentServiceHolderTest"/>
        </classes>
    </test>
    <test name="webhook-management-util-test">
        <classes>
            <class name="org.wso2.carbon.identity.webhook.management.util.WebhookValidatorTest"/>
            <class name="org.wso2.carbon.identity.webhook.management.util.WebhookSecretProcessorTest"/>
            <class name="org.wso2.carbon.identity.webhook.management.util.WebhookManagementAuditLoggerTest"/>
        </classes>
    </test>
</suite>
