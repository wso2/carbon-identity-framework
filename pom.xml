                <version>${commons.lang.version}</version>
            </dependency-->
            <dependency>
                <groupId>org.wso2.orbit.org.yaml</groupId>
                <artifactId>snakeyaml</artifactId>
                <version>${org.snakeyaml.version}</version>
            </dependency>

            <!--carbon deployment framework-->
            <dependency>
                <groupId>org.wso2.carbon.deployment</groupId>
                <artifactId>org.wso2.carbon.deployment.engine</artifactId>
                <version>${carbon.deployment.version}</version>
            </dependency>
            <dependency>
                <groupId>org.wso2.carbon.identity.mgt</groupId>
                <artifactId>org.wso2.carbon.identity.mgt</artifactId>
                <version>${carbon.identity.mgt.version}</version>
            </dependency>

        </dependencies>
    </dependencyManagement>


    <properties>
        <checkstyle.skip>true</checkstyle.skip>
        <findbugs.skip>true</findbugs.skip>

        <identity.gateway.version>${project.version}</identity.gateway.version>
        <identity.gateway.export.version>${identity.gateway.version}</identity.gateway.export.version>
        <identity.gateway.import.version.range>[1.0.0,2.0.0)</identity.gateway.import.version.range>

        <identity.gateway.api.version>${project.version}</identity.gateway.api.version>
        <identity.gateway.api.export.version>${identity.gateway.api.version}</identity.gateway.api.export.version>
        <identity.gateway.api.import.version.range>[1.0.0,2.0.0)</identity.gateway.api.import.version.range>

        <identity.gateway.common.version>${project.version}</identity.gateway.common.version>
        <identity.gateway.common.export.version>${identity.gateway.common.version}</identity.gateway.common.export.version>
        <identity.gateway.common.import.version.range>[1.0.0,2.0.0)</identity.gateway.common.import.version.range>

        <identity.gateway.resource.version>${project.version}</identity.gateway.resource.version>
        <identity.gateway.resource.export.version>${identity.gateway.resource.version}</identity.gateway.resource.export.version>
        <identity.gateway.resource.import.version.range>[1.0.0,2.0.0)</identity.gateway.resource.import.version.range>

        <carbon.identity.mgt.version>0.1.9</carbon.identity.mgt.version>
        <carbon.identity.mgt.import.version.range>[0.0.0,1.0.0)</carbon.identity.mgt.import.version.range>

        <!-- Carbon platform version comes here-->
        <carbon.kernel.version>5.1.0</carbon.kernel.version>
        <carbon.kernel.package.export.version>5.1.0</carbon.kernel.package.export.version>
        <carbon.kernel.package.import.version.range>[5.0.0, 6.0.0)</carbon.kernel.package.import.version.range>

        <carbon.identity.commons.version>0.1.13</carbon.identity.commons.version>
        <carbon.identity.common.import.version.range>[0.0.1, 1.0.0)
        </carbon.identity.common.import.version.range>

        <carbon.deployment.version>5.0.0</carbon.deployment.version>
        <carbon.deployment.import.version.range>[5.0.0, 6.0.0)</carbon.deployment.import.version.range>

        <osgi.core.api.version>6.0.0</osgi.core.api.version>
        <equinox.osgi.services.version>3.4.0.v20140312-2051</equinox.osgi.services.version>

        <osgi.framework.package.import.version.range>[1.8.0, 2.0.0)</osgi.framework.package.import.version.range>


        <msf4j.core.version>2.1.0</msf4j.core.version>
        <org.snakeyaml.version>1.16.0.wso2v1</org.snakeyaml.version>
