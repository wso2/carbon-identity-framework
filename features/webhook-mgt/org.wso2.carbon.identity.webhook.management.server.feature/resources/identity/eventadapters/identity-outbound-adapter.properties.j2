{% if outbound_adapters.websubhub.enabled is sameas true %}
adapter.websubhub.enabled={{outbound_adapters.websubhub.enabled}}
adapter.websubhub.baseUrl={{outbound_adapters.websubhub.base_url}}
adapter.websubhub.httpConnectionTimeout={{outbound_adapters.websubhub.httpConnectionTimeout}}
adapter.websubhub.httpReadTimeout={{outbound_adapters.websubhub.httpReadTimeout}}
adapter.websubhub.httpConnectionRequestTimeout={{outbound_adapters.websubhub.httpConnectionRequestTimeout}}
adapter.websubhub.connectionHandshakeTimeout={{outbound_adapters.websubhub.connectionHandshakeTimeout}}
adapter.websubhub.defaultMaxConnections={{outbound_adapters.websubhub.defaultMaxConnections}}
adapter.websubhub.defaultMaxConnectionsPerRoute={{outbound_adapters.websubhub.defaultMaxConnectionsPerRoute}}
adapter.websubhub.encryptionEnabled={{outbound_adapters.websubhub.encryptionEnabled}}
adapter.websubhub.encryptionKeyEndpointUrl={{outbound_adapters.websubhub.encryptionKeyEndpointUrl}}
adapter.websubhub.encryptionKeyCacheLifespan={{outbound_adapters.websubhub.encryptionKeyCacheLifespan}}
adapter.websubhub.topicDeletionDisabled={{outbound_adapters.websubhub.topicDeletionDisabled}}
adapter.websubhub.sslCertificateAlias={{outbound_adapters.websubhub.ssl_certificate_alias}}
adapter.websubhub.mtlsEnabled={{outbound_adapters.websubhub.mtlsEnabled}}
adapter.websubhub.type={{outbound_adapters.websubhub.type}}
{% endif %}

{% if outbound_adapters.httppublisher.enabled is sameas true %}
adapter.httppublisher.enabled={{outbound_adapters.httppublisher.enabled}}
adapter.httppublisher.httpConnectionTimeout={{outbound_adapters.httppublisher.httpConnectionTimeout}}
adapter.httppublisher.httpReadTimeout={{outbound_adapters.httppublisher.httpReadTimeout}}
adapter.httppublisher.httpConnectionRequestTimeout={{outbound_adapters.httppublisher.httpConnectionRequestTimeout}}
adapter.httppublisher.connectionHandshakeTimeout={{outbound_adapters.httppublisher.connectionHandshakeTimeout}}
adapter.httppublisher.defaultMaxConnections={{outbound_adapters.httppublisher.defaultMaxConnections}}
adapter.httppublisher.defaultMaxConnectionsPerRoute={{outbound_adapters.httppublisher.defaultMaxConnectionsPerRoute}}
adapter.httppublisher.type={{outbound_adapters.httppublisher.type}}
{% endif %}
